p1=input()
u=0
l=0
d=0
s=0
if (len(p1)>7):
    for i in p1:
        if(i.isupper()):
          u=u+1
        elif(i.islower()):
          l=l+1
        elif(i.isdigit()):
          d=d+1
        elif(not i.isalnum()):
          s=s+1
if (u>0 and l>0 and d>0 and s>0):
    print ("strong")
else:
    print("weak")
